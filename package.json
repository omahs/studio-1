        "@babel/preset-react": "^7.17.12",
        "@emotion/cache": "^11.4.0",
        "@emotion/react": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@hookform/resolvers": "1.3.5",
        "@mui/icons-material": "^5.6.1",
        "@mui/material": "^5.0.3",
        "@mui/styles": "^5.0.1",
        "@mui/system": "^5.0.3",
        "@mui/utils": "^5.0.1",
        "@tabler/icons": "^1.39.1",
        "apexcharts": "^3.26.1",
        "axios-mock-adapter": "^1.19.0",
        "chance": "^1.1.7",
        "date-fns": "^2.21.1",
        "formik": "^2.2.6",
        "framer-motion": "^4.1.13",
        "grapesjs": "^0.19.5",
        "jsonwebtoken": "^8.5.1",
        "jss": "^10.7.1",
        "jss-rtl": "^0.3.0",
        "jwt-decode": "^3.1.2",
        "material-icons": "^1.11.8",
        "material-ui-popup-state": "^1.8.0",
        "mixpanel-browser": "^2.45.0",
        "moralis-v1": "^1.12.0",
        "prop-types": "^15.7.2",
        "react": "^18.2.0",
        "react-apexcharts": "^1.3.7",
        "react-color": "^2.19.3",
        "react-compound-timer": "^1.2.0",
        "react-dappify": "^1.0.41",
        "react-device-detect": "^1.17.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^6.15.5",
        "react-identicons": "^1.2.5",
        "react-images": "^1.2.0-beta.6",
        "react-intersection-observer": "^8.32.0",
        "react-intl": "^5.14.0",
        "react-markdown": "^6.0.0",
        "react-moralis": "^1.4.2",
        "react-perfect-scrollbar": "^1.5.8",
        "react-redux": "^7.1.0-rc.1",
        "react-responsive-carousel": "^3.2.23",
        "react-router-dom": "^6.0.0-beta.5",
        "react-scripts": "^4.0.3",
        "react-share": "^4.4.0",
        "react-slick": "^0.28.1",
        "react-syntax-highlighter": "^15.4.3",
        "react-text-mask": "^5.4.3",
        "redux": "^4.0.5",
        "redux-persist": "^6.0.0",
        "remark-gfm": "^1.0.0",
        "stylis-plugin-rtl": "^2.0.2",
        "uuid": "^8.3.2",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "env-cmd": "^10.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "eslint": "^8.23.0",
        "eslint-config-react-app": "^7.0.1",
        "husky": "^8.0.1",
        "lint-staged": ">=10",
        "sass": "^1.56.2"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "coverageReporters": [
            "json",
            "text",
            "html",
            "lcov"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*/*.d.ts",
            "!src/**/*/Loadable.{js,jsx,ts,tsx}",
            "!src/**/*/types.ts",
            "!src/**/store.ts",
            "!src/index.tsx",
            "!src/serviceWorker.ts",
            "!<rootDir>/node_modules/",
            "!**/templates/**",
            "!**/template/**",
            "!src/__tests__/**",
            "!src/lib/assets/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 0.1,
                "functions": 0.1,
                "lines": 0.1,
                "statements": 0.1
            }
        }
    },
    "scripts": {
        "start": "export HTTPS=true&&SSL_CRT_FILE=cert.pem&&SSL_KEY_FILE=key.pem env-cmd -f .env react-scripts start",
        "build": "env-cmd -f .env react-scripts build",
        "build2": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__,node_modules",
        "start:cli": "react-scripts start",
        "build:cli": "react-scripts build",
        "test": "jest --collectCoverage=true",
        "coverage": "jest --coverage",
        "eject": "react-scripts eject",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    
    "lint-staged": {
        "*.js": [ 
            "eslint --cache --fix"
        ]
    },
    "resolutions": {
        "react": "^18.2.0"
    }
}
